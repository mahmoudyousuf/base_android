<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="rubikans.rubik.doctor.ui.main.home.HomeFragment"
        android:label="MainHomeFragment"
        tools:layout="@layout/fragment_home">


        <action
            android:id="@+id/openappointmentDetailsFragment"
            app:destination="@id/appointmentDetailsFragment">
            <argument
                android:name="appointmentId"
                app:argType="string" />

        </action>




        <action
            android:id="@+id/openMakeAppointmentFragment"
            app:destination="@id/makeAppointmentFragment">


        </action>


    </fragment>



    <fragment
        android:id="@+id/PatientFragment"
        android:name="rubikans.rubik.doctor.ui.main.patients.PatientsFragment"
        android:label="PatientFragment"
        tools:layout="@layout/fragment_patients">



    </fragment>




    <fragment
        android:id="@+id/changePasswordFragment"
        android:name="rubikans.rubik.doctor.ui.main.changePassword.ChangePasswordFragment"
        android:label="ChangePasswordFragment"
        tools:layout="@layout/fragment_change_password"
        />


    <fragment
        android:id="@+id/ProfileFragment"
        android:name="rubikans.rubik.doctor.ui.main.profile.ProfileFragment"
        android:label="ProfileFragment"
        tools:layout="@layout/fragment_profile">


        <action
            android:id="@+id/openChangePasswordFragment"
            app:destination="@id/changePasswordFragment" />





    </fragment>




    <fragment
        android:id="@+id/appointmentDetailsFragment"
        android:name="rubikans.rubik.doctor.ui.main.home.appointmentDetails.AppointmentDetailsFragment"
        android:label="appointmentDetailsFragment"
        tools:layout="@layout/fragment_appointment_details">

        <argument
            android:name="appointmentId"
            app:argType="string" />





    </fragment>




    <fragment
        android:id="@+id/makeAppointmentFragment"
        android:name="rubikans.rubik.doctor.ui.main.makeAppointment.MakeAppointmentFragment"
        android:label="makeAppointmentFragment"
        tools:layout="@layout/fragment_add_appointment">

    </fragment>



</navigation>